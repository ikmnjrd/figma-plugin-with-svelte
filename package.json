{
  "name": "figma-plugin-name",
  "version": "0.0.0",
  "description": "Figma Plugin Description",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm-run-all -p clean typecheck",
    "build": "npm-run-all -p build:*",
    "build:plugin": "node plugin/esbuild.mjs build",
    "build:ui": "vite build --config ui/vite.config.ts",
    "typecheck": "npm-run-all -p typecheck:*",
    "typecheck:plugin": "tsc -p plugin/tsconfig.json",
    "typecheck:ui": "tsc -p ui/tsconfig.json",
    "typecheck:ui-svelte": "svelte-check --tsconfig ./ui/tsconfig.json",
    "dev": "npm-run-all -p dev:*",
    "dev:plugin": "node plugin/esbuild.mjs watch",
    "dev:ui": "vite build --config ui/vite.config.ts --watch",
    "dev:ui-svelte": "svelte-check --tsconfig ./ui/tsconfig.json --watch",
    "fix": "npm-run-all -p fix:*",
    "fix:ui": "eslint -c ./eslint.config.mjs --fix . --debug",
    "lint": "npm-run-all -p lint:*",
    "lint:ui": "eslint -c ./eslint.config.mjs --debug"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@figma/eslint-plugin-figma-plugins": "^0.15.0",
    "@figma/plugin-typings": "^1.98.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^9.6.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-svelte": "^3.2.6",
    "rimraf": "^6.0.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "svelte-preprocess": "^6.0.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.1",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-plugin-svgr": "^4.2.0"
  }
}
